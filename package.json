{
  "name": "commander-backend",
  "version": "1.0.0",
  "description": "Backend services for commander application",
  "main": "app.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node bin/index.js",
    "build": "npm run deletedir && tsc && npm run move && npm run createdir",
    "move": "cp src/template/print.template.html bin/template",
    "createdir": "mkdir bin/receipt && cd bin/receipt && touch .gitkeep",
    "deletedir": "rm -fr bin/*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Julien Ouvrard",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-fileupload": "^0.4.0",
    "express-validator": "^5.3.1",
    "fast-csv": "^2.4.1",
    "html-pdf": "^2.2.0",
    "json2csv": "^4.3.3",
    "mongoose": "^5.4.11",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.25",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/debug": "0.0.30",
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.16.1",
    "@types/express-fileupload": "^0.1.1",
    "@types/express-validator": "^3.0.0",
    "@types/html-pdf": "^2.1.2",
    "@types/mongodb": "^3.1.19",
    "@types/mongoose": "^5.3.12",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.24",
    "@types/socket.io": "^2.1.2",
    "codelyzer": "^4.5.0",
    "eslint": "^4.19.1",
    "ts-node": "^6.2.0",
    "tslint": "^5.12.1",
    "typescript": "^2.9.2"
  }
}
